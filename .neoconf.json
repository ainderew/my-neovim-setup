{
  "neodev": {
    "library": {
      "enabled": true,
      "plugins": true
    }
  },
  "neoconf": {
    "plugins": {
      "lua_ls": {
        "enabled": true
      }
    }
  },
  "lspconfig": {
    "lua_ls": {
      "settings": {
        "Lua": {
          "workspace": {
            "checkThirdParty": false
          },
          "completion": {
            "callSnippet": "Replace"
          },
          "diagnostics": {
            "globals": ["vim"]
          }
        }
      }
    }
  }
}
